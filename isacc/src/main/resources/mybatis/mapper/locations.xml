<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="locations">
	<!-- SQL문들을 작성하는 파일 -->
	<select id="getAllLocations" resultType="locations.LocationsDTO">
		select * from locations l order by l.location_no
	</select>
</mapper>












